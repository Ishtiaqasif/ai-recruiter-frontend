            "post": {
                "tags": [
                    "Ingestion"
                ],
                "summary": "Ingest Document",
                "operationId": "ingest_document_ingest_post",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "$ref": "#/components/schemas/Body_ingest_document_ingest_post"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
