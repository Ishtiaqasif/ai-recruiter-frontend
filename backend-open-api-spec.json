        "/ingest/text": {
            "post": {
                "tags": [
                    "Ingestion"
                ],
                "summary": "Ingest Raw Text",
                "operationId": "ingest_text_ingest_text_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/IngestTextRequest"
